{
  "builders": [
    {
      "ami_block_device_mappings": [
        {
          "delete_on_termination": true,
          "device_name": "/dev/xvda",
          "encrypted": true,
          "volume_size": 8,
          "volume_type": "gp2"
        }
      ],
      "ami_name": "example-hvm-{{timestamp}}-x86_64-ebs",
      "ami_regions": "from-patcher-script",
      "instance_type": "t3.small",
      "launch_block_device_mappings": [
        {
          "delete_on_termination": true,
          "device_name": "/dev/xvda",
          "encrypted": true,
          "volume_size": 8,
          "volume_type": "gp2"
        }
      ],
      "region": "from-patcher-script",
      "region_kms_key_ids": {
        "from-patcher-script": "from-patcher-script"
      },
      "source_ami_filter": {
        "filters": {
          "name": "debian-10-amd64-*",
          "root-device-type": "ebs",
          "virtualization-type": "hvm"
        },
        "most_recent": true,
        "owners": [
          "136693071363"
        ]
      },
      "ssh_username": "admin",
      "tags": {
        "Application": "Example",
        "Base_AMI_Name": "{{ .SourceAMIName }}",
        "GitHub_SHA": "{{user `github_sha`}}",
        "OS_Version": "Debian Buster",
        "Pre_Release": "from-patcher-script",
        "Release": "{{user `image_version`}}",
        "Team": "CISA - Development"
      },
      "type": "amazon-ebs"
    }
  ],
  "provisioners": [
    {
      "groups": [
        "aws",
        "example"
      ],
      "playbook_file": "src/playbook.yml",
      "type": "ansible"
    }
  ],
  "sensitive-variables": [],
  "variables": {
    "github_sha": "{{env `GITHUB_SHA`}}",
    "image_version": "{{env `PACKER_IMAGE_VERSION`}}"
  }
}
